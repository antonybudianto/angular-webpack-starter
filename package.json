{
  "name": "angular-webpack-starter",
  "version": "2.0.0-beta.1",
  "author": "Antony Budianto",
  "description": "Angular Webpack Starter with extensible features",
  "main": "src/app/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonybudianto/angular-webpack-starter.git"
  },
  "engines": {
    "node": ">=4.3",
    "npm": "~3.0.0"
  },
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --inline --port 8080",
    "pretest": "npm run clean-coverage",
    "test": "karma start",
    "posttest": "npm run coverage",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "coverage": "npm run coverage:json && npm run coverage:html && npm run coverage:lcov",
    "coverage:init": "remap-istanbul -i coverage/coverage.json",
    "coverage:json": "npm run coverage:init -- -o coverage/coverage-final.json -t json",
    "coverage:html": "npm run coverage:init -- -o coverage/html-report -t html",
    "coverage:lcov": "npm run coverage:init -- -o coverage/lcov.info -t lcovonly",
    "ngc": "ngc -p tsconfig-aot.json",
    "serve": "lite-server -c config/browser-sync/bs-config.js",
    "prebuild": "npm run clean-build",
    "build": "npm run ngc && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "postbuild": "npm run clean-ngc",
    "serve-build": "npm run build && npm run serve",
    "clean": "npm run clean-build && npm run clean-ngc && npm run clean-coverage",
    "clean-build": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "clean-ngc": "rimraf output src/compiled",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "coveralls": "coveralls < ./coverage/lcov.info",
    "stat": "node ./config/dashboard"
  },
  "keywords": [
    "angular",
    "angular2",
    "starter",
    "seed",
    "webpack",
    "extensible",
    "travis",
    "karma",
    "jasmine",
    "istanbul"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/angular-webpack-starter/issues"
  },
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/compiler-cli": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.35",
    "@types/lodash": "4.14.37",
    "@types/node": "6.0.45",
    "angular2-router-loader": "^0.3.3",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "codelyzer": "1.0.0-beta.2",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "~2.5.2",
    "jquery": "^3.1.1",
    "karma": "~1.3.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "lodash": "^4.16.4",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "~0.7.0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-merge": "^0.15.0",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.2.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/protractor": "1.5.20",
    "@types/selenium-webdriver": "2.44.28",
    "compression": "^1.6.2",
    "lite-server": "^2.2.2",
    "protractor": "4.0.9",
    "ts-node": "^1.6.0",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
