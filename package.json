{
  "name": "angular-webpack-starter",
  "version": "2.0.0-alpha.1",
  "author": "Antony Budianto",
  "description": "Angular Webpack Starter with extensible features",
  "main": "src/app/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonybudianto/angular-webpack-starter.git"
  },
  "engines": {
    "node": ">=4.3",
    "npm": "~3.0.0"
  },
  "scripts": {
    "postinstall": "typings install",
    "start": "webpack-dashboard -- webpack-dev-server --inline --progress --port 8080",
    "pretest": "gulp clean-coverage",
    "test": "karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules",
    "coverage:report": "istanbul report",
    "env": "gulp env",
    "ngc": "ngc -p tsconfig-aot.json",
    "serve": "lite-server -c config/browser-sync/bs-config.js",
    "prebuild": "gulp clean-build",
    "build": "npm run ngc && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "postbuild": "gulp clean-ngc",
    "serve-build": "npm run build && npm run serve",
    "clean": "gulp clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "coveralls": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "angular",
    "angular2",
    "starter",
    "seed",
    "gulp",
    "webpack",
    "extensible",
    "travis",
    "karma",
    "jasmine",
    "istanbul"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/angular-webpack-starter/issues"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/compiler-cli": "0.6.2",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.2.33",
    "@types/lodash": "4.14.36",
    "@types/node": "6.0.40",
    "angular2-router-loader": "^0.2.2",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "bootstrap": "^3.3.7",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "del": "~2.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-template": "^4.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "~2.5.2",
    "jquery": "^3.1.1",
    "karma": "~1.3.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "lodash": "^4.15.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.12",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "~0.6.4",
    "require-dir": "~0.3.0",
    "rxjs": "5.0.0-beta.12",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "2.1.0-beta.25",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@types/protractor": "1.5.20",
    "@types/selenium-webdriver": "2.44.28",
    "compression": "^1.6.2",
    "glob": "^7.0.6",
    "lite-server": "^2.2.2",
    "protractor": "4.0.9",
    "ts-node": "^1.3.0",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^2.1.0-beta.5",
    "yargs": "^5.0.0"
  }
}